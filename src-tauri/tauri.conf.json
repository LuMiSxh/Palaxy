{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "pnpm run build",
    "beforeDevCommand": "pnpm run dev",
    "frontendDist": "../build",
    "devUrl": "http://localhost:5173"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "targets": "all",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    },
    "createUpdaterArtifacts": true
  },
  "productName": "Palaxy",
  "version": "0.2.0",
  "identifier": "Pala.Xy",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "hiddenTitle": true,
        "resizable": true,
        "title": "Kana Dock",
        "transparent": false,
        "minWidth": 1168,
        "minHeight": 552
      }
    ],
    "macOSPrivateApi": true,
    "security": {
      "csp": null,
      "capabilities": [
        {
          "identifier": "app",
            "description": "Capability for the core app",
            "windows": [
              "*"
            ],
            "permissions": [
              "core:app:allow-tauri-version",
              "core:app:allow-version",
              "core:app:allow-name"
            ]
        },
        {
          "identifier": "dialog",
          "description": "Capability for the dialog plugin",
          "windows": [
            "*"
          ],
          "permissions": [
            "dialog:allow-open",
            "dialog:deny-ask",
            "dialog:deny-confirm",
            "dialog:deny-save"
          ]
        }
      ],
      "assetProtocol": {
        "enable": true,
        "scope": {
          "allow": [
            "**"
          ]
        }
      }
    }
  }
}